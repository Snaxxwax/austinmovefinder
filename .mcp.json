{
  "mcpServers": {
    "cloudflare-browser-rendering": {
      "type": "sse",
      "url": "https://browser.mcp.cloudflare.com/sse"
    },
    "cloudflare-workers-builds": {
      "type": "sse",
      "url": "https://builds.mcp.cloudflare.com/sse"
    },
    "cloudflare-workers-bindings": {
      "type": "sse",
      "url": "https://bindings.mcp.cloudflare.com/sse"
    },
    "context7": {
      "type": "stdio",
      "command": "npx",
      "args": [
        "-y",
        "@upstash/context7-mcp@latest"
      ],
      "env": {}
    },
    "playwright": {
      "type": "stdio",
      "command": "npx",
      "args": [
        "@playwright/mcp@latest"
      ],
      "env": {}
    },
    "astro-docs": {
      "type": "http",
      "url": "https://mcp.docs.astro.build/mcp"
    },
    "claude-flow": {
      "type": "stdio",
      "command": "npx",
      "args": [
        "claude-flow@alpha",
        "mcp",
        "start"
      ],
      "env": {}
    },
    "ruv-swarm": {
      "type": "stdio",
      "command": "npx",
      "args": [
        "ruv-swarm",
        "mcp",
        "start"
      ],
      "env": {}
    },
    "flow-nexus": {
      "type": "stdio",
      "command": "npx",
      "args": [
        "flow-nexus@latest",
        "mcp",
        "start"
      ],
      "env": {}
    }
  }
}