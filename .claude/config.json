{
  "version": "1.0",
  "project": {
    "name": "Austin Move Finder",
    "type": "react-typescript-vite",
    "framework": "React + TypeScript + Tailwind",
    "domain": "Austin moving services",
    "commands": {
      "dev": "npm run dev",
      "build": "npm run build",
      "lint": "npm run lint",
      "test": "npm run test"
    }
  },
  "sparc": {
    "enabled": true,
    "modes": [
      "orchestrator",
      "coder",
      "researcher",
      "tdd",
      "architect",
      "reviewer",
      "debugger",
      "tester",
      "analyzer",
      "optimizer",
      "documenter",
      "designer",
      "innovator",
      "swarm-coordinator",
      "memory-manager",
      "batch-executor",
      "workflow-manager"
    ]
  },
  "swarm": {
    "enabled": true,
    "strategies": [
      "research",
      "development",
      "analysis",
      "testing",
      "optimization",
      "maintenance"
    ]
  },
  "context": {
    "austin_focus": true,
    "tech_stack": ["React", "TypeScript", "Tailwind", "Vite", "shadcn/ui"],
    "key_directories": [
      "src/components",
      "src/pages",
      "src/lib",
      "src/data"
    ],
    "data_files": [
      "src/lib/utils.ts"
    ]
  }
}