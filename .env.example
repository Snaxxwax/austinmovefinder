# Austin Move Finder Frontend Configuration
# Copy this file to .env.local and fill in your values

# Application
VITE_APP_NAME=Austin Move Finder
VITE_APP_URL=http://localhost:5173

# Backend API Configuration
VITE_BACKEND_URL=http://localhost:5000
# VITE_BACKEND_URL=https://api.austinmovefinder.com  # Production

# Production URLs (set these in Cloudflare Pages)
# VITE_APP_URL=https://austinmovefinder.pages.dev
# VITE_BACKEND_URL=https://api.austinmovefinder.com

# Hugging Face API Configuration
# Get your API key from: https://huggingface.co/settings/tokens
VITE_HUGGINGFACE_API_KEY=your_huggingface_api_key_here

# Optional: Custom object detection model endpoint
# VITE_CUSTOM_DETECTION_ENDPOINT=https://your-custom-endpoint.com

# Development settings
VITE_ENABLE_OBJECT_DETECTION=true
VITE_MAX_DETECTION_FILES=10
VITE_MAX_FILE_SIZE_MB=25

# Austin moving service configuration
VITE_BASE_LABOR_RATE=120
VITE_TRUCK_RATE_PER_MILE=1.2

# EmailJS Configuration (for sending quote emails)
# Get these from: https://dashboard.emailjs.com/
VITE_EMAILJS_SERVICE_ID=your_service_id_here
VITE_EMAILJS_TEMPLATE_ID=your_template_id_here
VITE_EMAILJS_PUBLIC_KEY=your_public_key_here

# Analytics (optional)
# VITE_GA_TRACKING_ID=G-XXXXXXXXXX
# VITE_PLAUSIBLE_DOMAIN=austinmovefinder.com

# Contact Form (if using a service)
# VITE_CONTACT_FORM_ENDPOINT=https://formspree.io/f/your-form-id

# Social Media (for sharing)
# VITE_TWITTER_HANDLE=@austinmovefinder
# VITE_FACEBOOK_PAGE=austinmovefinder