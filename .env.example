# Cloudflare Environment Variables
# Copy this to .env and fill in your values

# Turnstile (Bot Protection)
PUBLIC_TURNSTILE_SITE_KEY=your_turnstile_site_key_here
TURNSTILE_SECRET_KEY=your_turnstile_secret_key_here

# Email Configuration
EMAIL_API_KEY=your_email_api_key_here
EMAIL_FROM=quotes@austinmovefinder.com
EMAIL_TO=admin@austinmovefinder.com

# R2 Storage
R2_BUCKET_NAME=austin-move-finder-submissions

# Optional: Analytics
PUBLIC_CLOUDFLARE_ANALYTICS_TOKEN=your_analytics_token_here